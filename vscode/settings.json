    "journal.templates": [
        {
            "name": "memo",
            "template": "- MEMO ${localTime}: ${input}"
        },
        {
            "name": "task",
            "template": "- [] ${d:LL} - Task: ${input}"
        },
        {
            "name": "entry",
            "template": "# ${d:dddd, MMMM DD YYYY}\n\n## Tasks\n\n## Notes\n\n"
        },
        {
            "name": "time",
            "template": "${localTime}"
        },
        {
            "name": "note",
            "template": "# ${input}\n\n${tags}\n"
        },
        {
            "name": "files",
            "template": "- NOTE: [${title}](${link})"
        }
    ],
    "pandoc.htmlOptString": "-s -f gfm -t html5 --css=style.css",
    "remote.SSH.showLoginTerminal": true,
    "remote.SSH.useLocalServer": false,
    "remote.SSH.remotePlatform": {
        "192.168.0.21": "linux",
        "192.168.0.15": "linux"
    },
    "editor.accessibilitySupport": "off",
    "todo-tree.tree.showScanModeButton": false,
